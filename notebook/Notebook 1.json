					"       AVG(trip_distance) as AvgTripDistance\r\n",
					"   FROM default.trip\r\n",
					"   WHERE trip_distance > 0 AND passenger_count > 0\r\n",
					"   GROUP BY passenger_count\r\n",
					"   ORDER BY passenger_count\r\n",
					"\"\"\") \r\n",
					"display(df)\r\n",
